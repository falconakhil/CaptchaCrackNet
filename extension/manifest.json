{
    "manifest_version": 3,
    "name": "Captcha Solver",
    "version": "1.0",
    "description": "Solves text CAPTCHAs using ML",
    "permissions": [
        "scripting", 
        "activeTab"
        ],
  "host_permissions": [
    "<all_urls>"
  ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
}